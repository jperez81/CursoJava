package poo;

public class Uso_Choche {

	public static void main(String[] args) {

		Coche Renault = new Coche(); // Instanciar la clase le damos el nombre al objeto y llamamos al constructor ()
		
		System.out.println("La cantidad de Ruedas que tiene es : " + Renault.getRuedas() + " ruedas");
		System.out.println("El largo del coche es : " + Renault.getLargo() + " Cm");

	}
}
